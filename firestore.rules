rules_version = '2';
service cloud.firestore {
    match /databases/{database}/documents {
    function isAuthorized() {
      return request.auth != null;
    }

    match /{document=**} {
      allow read, write, update: if isAuthorized();
    }

    match /users/id {
      allow read, write, update: if isAuthorized();
    }
  }
}